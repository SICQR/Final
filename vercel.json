{
  "version": 2,
  "name": "hotmess-editorial-commerce",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/admin",
      "dest": "/admin",
      "headers": {
        "cache-control": "no-cache"
      }
    }
  ],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}